{
  "name": "msgee",
  "version": "2.0.1",
  "description": "An extended events here",
  "author": "Konstantin Buza",
  "license": "MIT",
  "repository": "github:buza-me/msgee",
  "main": "dist/lib/main.js",
  "types": "dist/lib/types/main.d.ts",
  "browser": "dist/bundle.browser.iife.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --outDir dist/lib --declarationDir dist/types --declaration true && rollup -c rollup.config.js",
    "lint-fix": "eslint ./ --fix",
    "lint": "eslint ./",
    "preversion": "sh ./preversion.sh",
    "version": "sh ./version.sh",
    "publish": "sh ./publish.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint-fix",
      "npm run lint"
    ]
  }
}
